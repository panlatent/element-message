{
    "name": "panlatent/element-messages",
    "description": "Element Messages for CraftCMS 3",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "element",
        "messages"
    ],
    "support": {
        "docs": "https://github.com/panlatent/element-messages/blob/master/README.md",
        "issues": "https://github.com/panlatent/element-messages/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "panlatent@gmail.com",
            "homepage": "https://panlatent.com/"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^3.1|^4.0"
    },
    "autoload": {
        "psr-4": {
          "panlatent\\elementmessages\\": "src/"
        }
    },
    "extra": {
        "name": "Element Messages",
        "handle": "elementmessages",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/panlatent/element-messages/master/CHANGELOG.md",
        "class": "panlatent\\elementmessages\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
